{
    "name": "ahnames/hiam",
    "type": "yii2-extension",
    "description": "HIAM for AHnames",
    "keywords": [
        "ahnames",
        "hiam",
        "login",
        "access management"
    ],
    "homepage": "https://github.com/ahnames/hiam",
    "license": "proprietary",
    "support": {
        "email": "support@ahnames.com",
        "source": "https://github.com/ahnames/hiam",
        "issues": "https://github.com/ahnames/hiam/issues",
        "wiki": "https://github.com/ahnames/hiam/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        }
    ],
    "require": {
        "php": "^5.5|^7.0",
        "ahnames/yii2-asset-ahnames": "*",
        "hiqdev/hiam-core": "*"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "ahnames/hidev-vendor": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "ahnames\\hiam\\": "src"
        }
    },
    "extra": {
        "extension-plugin": {
            "hisite-config": "src/hisite-config.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.hiqdev.com"
        }
    ]
}
